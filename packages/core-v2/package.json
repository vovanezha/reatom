{
  "name": "@reatom/core-v2",
  "version": "3.1.2",
  "private": false,
  "sideEffects": false,
  "description": "Reatom adapter for core-v2",
  "source": "src/index.ts",
  "main": "build/index.js",
  "module": "build/index.es.js",
  "types": "build/index.d.ts",
  "browserslist": [
    "last 1 year"
  ],
  "minify": {
    "mangle": false
  },
  "scripts": {
    "prepublishOnly": "npm run build && npm run build-primitives && npm run test",
    "build": "microbundle -f esm,cjs",
    "build-primitives": "microbundle --cwd primitives",
    "test": "echo tsx src/index.test.ts",
    "test:watch": "tsx watch src/index.test.ts"
  },
  "dependencies": {
    "@reatom/core": "^3.1.0"
  },
  "author": "artalar",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/artalar/reatom.git"
  },
  "bugs": {
    "url": "https://github.com/artalar/reatom/issues"
  },
  "homepage": "https://www.reatom.dev/packages/core-v2",
  "files": [
    "/build",
    "/package.json",
    "/primitives"
  ]
}
