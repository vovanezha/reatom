{
  "name": "@reatom/npm-cookie-baker",
  "version": "3.1.3",
  "private": false,
  "sideEffects": false,
  "description": "Reatom for cookies by cookie-baker",
  "source": "src/index.ts",
  "exports": {
    "require": "./build/index.js",
    "default": "./build/index.module.js",
    "types": "./build/index.d.ts"
  },
  "main": "build/index.js",
  "module": "build/index.module.js",
  "types": "build/index.d.ts",
  "browserslist": [
    "last 1 year"
  ],
  "minify": {
    "mangle": false
  },
  "scripts": {
    "prepublishOnly": "npm run build && npm run test",
    "build": "microbundle -f esm,cjs",
    "test": "tsx src/index.test.ts",
    "test:watch": "tsx watch src/index.test.ts"
  },
  "dependencies": {
    "@reatom/core": "^3.1.0",
    "@reatom/hooks": "^3.1.2",
    "@cookie-baker/core": "^0.2.1"
  },
  "author": "BataevDaniil",
  "maintainers": [
    {
      "name": "artalar"
    },
    {
      "name": "BataevDaniil"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/artalar/reatom.git"
  },
  "bugs": {
    "url": "https://github.com/artalar/reatom/issues"
  },
  "homepage": "https://www.reatom.dev/packages/npm-cookie-baker",
  "files": [
    "/build",
    "/package.json"
  ]
}
